.beauty_parlor_container{
  height: 200rpx;
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border:1rpx solid rgba(242, 242, 242, 1);
}
.beauty_left{
  width: 158rpx;
  height: 158rpx;
  margin-right: 20rpx;
  flex-grow: 0;
  flex-shrink: 0;
}
.beauty_right{
  height: 100%;
  flex-shrink: 1;
  flex-grow: 1;
}
.beauty_parlor_name{
  font-size: 32rpx;
  padding: 2rpx 0 10rpx 0;
  display: block;
}
.beauty_area{
  width: 100%;
  display: flex;
  justify-content: space-between
}
.area_icon{
  width: 24rpx;
  height: 40rpx;
  flex-grow: 0;
  margin-right: 16rpx;
  flex-shrink: 0;
}
.area_text{
  font-size: 24rpx;
  flex-shrink: 1;
  flex-grow: 1;
}
.phone_icon{
  width: 80rpx;
  height: 80rpx;
  margin-left: 50rpx;
  flex-grow: 0;
  flex-shrink: 0;
}