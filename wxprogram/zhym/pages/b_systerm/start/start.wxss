.container{
  text-align: center;
}
.policy{
  font-size: 32rpx;
}
.lemma{
  font-size: 36rpx;
  padding: 80rpx 0;
}
.main_business{
  width: 100%;
  padding: 0 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.main_business text{
  display: inline-block;
  flex-shrink: 0;
  flex-grow: 0;
  width: 156rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 24rpx;
  margin-bottom: 40rpx;
  background: rgba(242, 242, 242, 1);
}