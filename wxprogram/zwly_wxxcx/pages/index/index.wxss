/**index.wxss**/
page{
    position: relative;
}
.container {
    overflow-y: auto;
}
.container > view{
    width: 100%;
}
.container-msg{
    flex: 1;
    overflow: auto;
    -webkit-overflow-scrolling : touch;
    margin-top: 104rpx;
}

/* 轮播图 */

.swiper-container {
    position: relative;
    /* margin-top: 104rpx; */
}

.swiper-container .swiper {
    height: 360rpx;
}

.swiper-container .swiper .img {
    width: 100%;
    height: 100%;
}

.swiper-container .dots {
    position: absolute;
    left: 0;
    right: 0;
    bottom: -18rpx;
    display: flex;
    justify-content: center;
}

.swiper-container .dots .dot {
    margin: 0 3rpx;
    width: 8rpx;
    height: 8rpx;
    background: #E7E7E7;
    border-radius: 100rpx;
    transition: all 0.6s;
}

.swiper-container .dots .dot.active {
    width: 40rpx;
    background: #FE4E4E;
}
/* 公共标题 */
.public-title{
    display: flex;
    flex-direction: row;
    align-items: center;
    padding: 0 30rpx;
    margin-bottom: 20rpx;
}
.public-title text{
    flex: 1;
    font-size: 40rpx;
    color: #2D2D2D;
    line-height: 56rpx;
}
.public-title view{
    width: 28.3rpx;
    box-sizing: border-box;
    padding-left: 14.15rpx;
}
.public-title view image{
    width: 100%;
    height: 28.3rpx;
}
/* 打折专区 */
.discount{
    margin-top: 67rpx;
}
.discount .discount-msg{
    height: 351rpx;
    background: #fff;
    margin: 0 30rpx;
