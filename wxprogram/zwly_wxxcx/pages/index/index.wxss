}
.discount .discount-msg .discount-item .zk-zi text{
    font-size: 24rpx;
    display: block;
}
.discount .discount-msg .discount-items{
    border-left: 1rpx solid #F7F7F7;
}
.discount .discount-msg .discount-items image{
    width: 170rpx;
    height: 170rpx;
